#include <stdio.h>

int main()
{
   char str[100];
   int vowelcount=0,wordcount=0,i,len;
   gets(str);
len=strlen(str);
for(i=0;i<=len;i++){
    if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
    vowelcount++;
    if(str[i]==' '||str[i]=='\0')
    wordcount++;
}
printf("\n %d vowels",vowelcount);
printf("\n %d words",wordcount);
    return 0;
}

